{
  "version": 2,
  "name": "spend-track-testing-reports",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "playwright-report"
      }
    }
  ],
  "buildCommand": "yarn build-reports:script",
  "outputDirectory": "playwright-report",
  "installCommand": "yarn install",
  "framework": null,
  "public": true,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
